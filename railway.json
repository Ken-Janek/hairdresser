{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE"
  },
  "deploy": {
    "numReplicas": 1,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10,
    "sleepBeforeRestart": 10
  },
  "variables": {
    "PATH_ADD": "./public",
    "PHP_VERSION": "8.3"
  }
}
